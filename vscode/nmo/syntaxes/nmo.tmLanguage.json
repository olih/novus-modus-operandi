{
  "$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
  "name": "Novus Modus Operandi",
  "patterns": [
    {
      "include": "#header"
    },
    {
      "include": "#fragments"
    }
  ],
  "repository": {
    "header": {
      "patterns": [
        {
          "comment": "section header",
          "match": "^(section header)$",
          "captures": {
            "1": {
              "name": "markup.heading.1.nmo"
            }
          }
        },
        {
          "comment": "id-urn colon-separator id-path",
          "match": "^(id-urn)[ ]*(:)[ ]*([^\\s:,;|@%^&*()+=]+)$",
          "captures": {
            "1": {
              "name": "entity.name.tag.nmo"
            },
            "2": {
              "name": "punctuation.definition.separator.nmo"
            },
            "3": {
              "name": "markup.italic.nmo"
            }
          }
        },
        {
          "comment": "require-sections colon-separator require-sections-values",
          "match": "^(require-sections)[ ]*(:)[ ]*([\\[][^\\[]*[\\]])$",
          "captures": {
            "1": {
              "name": "entity.name.tag.nmo"
            },
            "2": {
              "name": "punctuation.definition.separator.nmo"
            },
            "3": {
              "patterns": [
                {
                  "comment": "separator",
                  "match": "\\b[,]\\s?",
                  "name": "punctuation.separator.array.nmo"
                },
                {
                  "comment": "require-sections section-version",
                  "match": "(accessors|chunks|fragments|header)[ ]*([0-9]+[.][0-9]+[.][0-9]+)",
                  "captures": {
                    "1": {
                      "name": "keyword.control.nmo"
                    },
                    "2": {
                      "name": "constant.numeric.nmo"
                    }
                  }
                }
              ]
            }
          }
        },
        {
          "comment": "prefixes colon-separator prefixes-values",
          "match": "^(prefixes)[ ]*(:)[ ]*([\\[][^\\[]*[\\]])$",
          "captures": {
            "1": {
              "name": "entity.name.tag.nmo"
            },
            "2": {
              "name": "punctuation.definition.separator.nmo"
            },
            "3": {
              "patterns": [
                {
                  "comment": "separator",
                  "match": "\\b[,]\\s?",
                  "name": "punctuation.separator.array.nmo"
                },
                {
                  "comment": "prefix prefix-url",
                  "match": "([a-z][a-z0-9-]+)[ ]*(https?://[^\\s]+)",
                  "captures": {
                    "1": {
                      "name": "variable.parameter.nmo"
                    },
                    "2": {
                      "name": "markup.underline.link.nmo"
                    }
                  }
                }
              ]
            }
          }
        },
        {
          "comment": "name language colon-separator name-value",
          "match": "^(name)[ ]*([a-z]{2}(-[a-z]{2, 3})?)[ ]*(:)[ ]*(.{2,})$",
          "captures": {
            "1": {
              "name": "entity.name.tag.nmo"
            },
            "2": {
              "name": "constant.other.symbol.nmo"
            },
            "3": {
              "name": "punctuation.definition.separator.nmo"
            },
            "4": {
              "name": "string.nmo"
            }
          }
        },
        {
          "comment": "title language colon-separator title-value",
          "match": "^(title)[ ]*([a-z]{2}(-[a-z]{2, 3})?)[ ]*(:)[ ]*(.{2,})$",
          "captures": {
            "1": {
              "name": "entity.name.tag.nmo"
            },
            "2": {
              "name": "constant.other.symbol.nmo"
            },
            "3": {
              "name": "punctuation.definition.separator.nmo"
            },
            "4": {
              "name": "string.nmo"
            }
          }
        },
        {
          "comment": "license language colon-separator license-value",
          "match": "^(license)[ ]*([a-z]{2}(-[a-z]{2, 3})?)[ ]*(:)[ ]*(.{2,})$",
          "captures": {
            "1": {
              "name": "entity.name.tag.nmo"
            },
            "2": {
              "name": "constant.other.symbol.nmo"
            },
            "3": {
              "name": "punctuation.definition.separator.nmo"
            },
            "4": {
              "name": "string.nmo"
            }
          }
        },
        {
          "comment": "attribution-name language colon-separator attribution-name-value",
          "match": "^(attribution-name)[ ]*([a-z]{2}(-[a-z]{2, 3})?)[ ]*(:)[ ]*(.{2,})$",
          "captures": {
            "1": {
              "name": "entity.name.tag.nmo"
            },
            "2": {
              "name": "constant.other.symbol.nmo"
            },
            "3": {
              "name": "punctuation.definition.separator.nmo"
            },
            "4": {
              "name": "string.nmo"
            }
          }
        },
        {
          "comment": "author language colon-separator author-value",
          "match": "^(author)[ ]*([a-z]{2}(-[a-z]{2, 3})?)[ ]*(:)[ ]*(.{2,})$",
          "captures": {
            "1": {
              "name": "entity.name.tag.nmo"
            },
            "2": {
              "name": "constant.other.symbol.nmo"
            },
            "3": {
              "name": "punctuation.definition.separator.nmo"
            },
            "4": {
              "name": "string.nmo"
            }
          }
        },
        {
          "comment": "description language colon-separator description-value",
          "match": "^(description)[ ]*([a-z]{2}(-[a-z]{2, 3})?)[ ]*(:)[ ]*(.{2,})$",
          "captures": {
            "1": {
              "name": "entity.name.tag.nmo"
            },
            "2": {
              "name": "constant.other.symbol.nmo"
            },
            "3": {
              "name": "punctuation.definition.separator.nmo"
            },
            "4": {
              "name": "string.nmo"
            }
          }
        },
        {
          "comment": "license-url mediatype language colon-separator license-url-value",
          "match": "^(license-url)[ ]*(csv|html|json|json-ld|markdown|nt|rdf|ttl)[ ]*([a-z]{2}(-[a-z]{2, 3})?)[ ]*(:)[ ]*(https?://[^\\s]+)$",
          "captures": {
            "1": {
              "name": "entity.name.tag.nmo"
            },
            "2": {
              "name": "keyword.control.nmo"
            },
            "3": {
              "name": "constant.other.symbol.nmo"
            },
            "4": {
              "name": "punctuation.definition.separator.nmo"
            },
            "5": {
              "name": "markup.underline.link.nmo"
            }
          }
        },
        {
          "comment": "author-url mediatype language colon-separator author-url-value",
          "match": "^(author-url)[ ]*(csv|html|json|json-ld|markdown|nt|rdf|ttl)[ ]*([a-z]{2}(-[a-z]{2, 3})?)[ ]*(:)[ ]*(https?://[^\\s]+)$",
          "captures": {
            "1": {
              "name": "entity.name.tag.nmo"
            },
            "2": {
              "name": "keyword.control.nmo"
            },
            "3": {
              "name": "constant.other.symbol.nmo"
            },
            "4": {
              "name": "punctuation.definition.separator.nmo"
            },
            "5": {
              "name": "markup.underline.link.nmo"
            }
          }
        },
        {
          "comment": "license-url mediatype language colon-separator license-url-value",
          "match": "^(license-url)[ ]*(csv|html|json|json-ld|markdown|nt|rdf|ttl)[ ]*([a-z]{2}(-[a-z]{2, 3})?)[ ]*(:)[ ]*(https?://[^\\s]+)$",
          "captures": {
            "1": {
              "name": "entity.name.tag.nmo"
            },
            "2": {
              "name": "keyword.control.nmo"
            },
            "3": {
              "name": "constant.other.symbol.nmo"
            },
            "4": {
              "name": "punctuation.definition.separator.nmo"
            },
            "5": {
              "name": "markup.underline.link.nmo"
            }
          }
        },
        {
          "comment": "attribution-url mediatype language colon-separator attribution-url-value",
          "match": "^(attribution-url)[ ]*(csv|html|json|json-ld|markdown|nt|rdf|ttl)[ ]*([a-z]{2}(-[a-z]{2, 3})?)[ ]*(:)[ ]*(https?://[^\\s]+)$",
          "captures": {
            "1": {
              "name": "entity.name.tag.nmo"
            },
            "2": {
              "name": "keyword.control.nmo"
            },
            "3": {
              "name": "constant.other.symbol.nmo"
            },
            "4": {
              "name": "punctuation.definition.separator.nmo"
            },
            "5": {
              "name": "markup.underline.link.nmo"
            }
          }
        },
        {
          "comment": "homepage-url mediatype language colon-separator homepage-url-value",
          "match": "^(homepage-url)[ ]*(csv|html|json|json-ld|markdown|nt|rdf|ttl)[ ]*([a-z]{2}(-[a-z]{2, 3})?)[ ]*(:)[ ]*(https?://[^\\s]+)$",
          "captures": {
            "1": {
              "name": "entity.name.tag.nmo"
            },
            "2": {
              "name": "keyword.control.nmo"
            },
            "3": {
              "name": "constant.other.symbol.nmo"
            },
            "4": {
              "name": "punctuation.definition.separator.nmo"
            },
            "5": {
              "name": "markup.underline.link.nmo"
            }
          }
        },
        {
          "comment": "repository-url mediatype language colon-separator repository-url-value",
          "match": "^(repository-url)[ ]*(csv|html|json|json-ld|markdown|nt|rdf|ttl)[ ]*([a-z]{2}(-[a-z]{2, 3})?)[ ]*(:)[ ]*(https?://[^\\s]+)$",
          "captures": {
            "1": {
              "name": "entity.name.tag.nmo"
            },
            "2": {
              "name": "keyword.control.nmo"
            },
            "3": {
              "name": "constant.other.symbol.nmo"
            },
            "4": {
              "name": "punctuation.definition.separator.nmo"
            },
            "5": {
              "name": "markup.underline.link.nmo"
            }
          }
        },
        {
          "comment": "copyright-year colon-separator copyright-year-value",
          "match": "^(copyright-year)[ ]*(:)[ ]*([+-]?[0-9]+)$",
          "captures": {
            "1": {
              "name": "entity.name.tag.nmo"
            },
            "2": {
              "name": "punctuation.definition.separator.nmo"
            },
            "3": {
              "name": "constant.numeric.nmo"
            }
          }
        },
        {
          "comment": "require-types colon-separator type-arr",
          "match": "^(require-types)[ ]*(:)[ ]*([\\[][^\\[]*[\\]])$",
          "captures": {
            "1": {
              "name": "entity.name.tag.nmo"
            },
            "2": {
              "name": "punctuation.definition.separator.nmo"
            },
            "3": {
              "patterns": [
                {
                  "comment": "separator",
                  "match": "\\b[,]\\s?",
                  "name": "punctuation.separator.array.nmo"
                },
                {
                  "comment": "require-type",
                  "match": "(csvenum|email|enum|float|fraction|freetext|idstring|int|marker|time)",
                  "captures": {
                    "1": {
                      "name": "keyword.control.nmo"
                    }
                  }
                }
              ]
            }
          }
        },
        {
          "comment": "require-generators colon-separator generator-arr",
          "match": "^(require-generators)[ ]*(:)[ ]*([\\[][^\\[]*[\\]])$",
          "captures": {
            "1": {
              "name": "entity.name.tag.nmo"
            },
            "2": {
              "name": "punctuation.definition.separator.nmo"
            },
            "3": {
              "patterns": [
                {
                  "comment": "separator",
                  "match": "\\b[,]\\s?",
                  "name": "punctuation.separator.array.nmo"
                },
                {
                  "comment": "require-generators",
                  "match": "(elm-string-serializer|python-string-serializer|python-string-validator|slack-ui|vscode-syntax)",
                  "captures": {
                    "1": {
                      "name": "keyword.control.nmo"
                    }
                  }
                }
              ]
            }
          }
        },
        {
          "comment": "require-string-constraints colon-separator string-constraint-arr",
          "match": "^(require-string-constraints)[ ]*(:)[ ]*([\\[][^\\[]*[\\]])$",
          "captures": {
            "1": {
              "name": "entity.name.tag.nmo"
            },
            "2": {
              "name": "punctuation.definition.separator.nmo"
            },
            "3": {
              "patterns": [
                {
                  "comment": "separator",
                  "match": "\\b[,]\\s?",
                  "name": "punctuation.separator.array.nmo"
                },
                {
                  "comment": "string-constraint",
                  "match": "(AZ|az|char-exp|dash|digit|ends-with|followed-by|hash:sha-256|max-length|min-length|multiple-lines|single-line|starts-with|underscore)",
                  "captures": {
                    "1": {
                      "name": "keyword.control.nmo"
                    }
                  }
                }
              ]
            }
          }
        },
        {
          "comment": "require-number-constraints colon-separator number-constraint-arr",
          "match": "^(require-number-constraints)[ ]*(:)[ ]*([\\[][^\\[]*[\\]])$",
          "captures": {
            "1": {
              "name": "entity.name.tag.nmo"
            },
            "2": {
              "name": "punctuation.definition.separator.nmo"
            },
            "3": {
              "patterns": [
                {
                  "comment": "separator",
                  "match": "\\b[,]\\s?",
                  "name": "punctuation.separator.array.nmo"
                },
                {
                  "comment": "number-constraint",
                  "match": "(< _ <|< _ <=|<= _ <|<= _ <=|a < b|multiple-of|zero-pad)",
                  "captures": {
                    "1": {
                      "name": "keyword.control.nmo"
                    }
                  }
                }
              ]
            }
          }
        },
        {
          "comment": "require-list-constraints colon-separator list-constraint-arr",
          "match": "^(require-list-constraints)[ ]*(:)[ ]*([\\[][^\\[]*[\\]])$",
          "captures": {
            "1": {
              "name": "entity.name.tag.nmo"
            },
            "2": {
              "name": "punctuation.definition.separator.nmo"
            },
            "3": {
              "patterns": [
                {
                  "comment": "separator",
                  "match": "\\b[,]\\s?",
                  "name": "punctuation.separator.array.nmo"
                },
                {
                  "comment": "list-constraint",
                  "match": "(max-items|min-items)",
                  "captures": {
                    "1": {
                      "name": "keyword.control.nmo"
                    }
                  }
                }
              ]
            }
          }
        },
        {
          "comment": "require-access-constraints colon-separator access-constraint-arr",
          "match": "^(require-access-constraints)[ ]*(:)[ ]*([\\[][^\\[]*[\\]])$",
          "captures": {
            "1": {
              "name": "entity.name.tag.nmo"
            },
            "2": {
              "name": "punctuation.definition.separator.nmo"
            },
            "3": {
              "patterns": [
                {
                  "comment": "separator",
                  "match": "\\b[,]\\s?",
                  "name": "punctuation.separator.array.nmo"
                },
                {
                  "comment": "access-constraint",
                  "match": "(daily-read|daily-write|editable|hourly-read|hourly-write|monthly-read|monthly-write|read-once|remote-check|write-once)",
                  "captures": {
                    "1": {
                      "name": "keyword.control.nmo"
                    }
                  }
                }
              ]
            }
          }
        }
      ]
    },
    "fragments": {
      "patterns": [
        {
          "comment": "section fragments",
          "match": "^(section fragments)$",
          "captures": {
            "1": {
              "name": "markup.heading.1.nmo"
            }
          }
        },
        {
          "comment": "fragment fragment-id marker constraints empty-array values values-arr arrow-separator other",
          "match": "^(fragment)[ ]*([a-z][a-z0-9-]+)[ ]*(marker)[ ]*(constraints)[ ]*(\\[\\])[ ]*(values)[ ]*([\\[][^\\[]*[\\]])[ ]*(->)[ ]*(.{2,})$",
          "captures": {
            "1": {
              "name": "entity.name.tag.nmo"
            },
            "2": {
              "name": "variable.parameter.nmo"
            },
            "3": {
              "name": "entity.name.tag.nmo"
            },
            "4": {
              "name": "entity.name.tag.nmo"
            },
            "5": {
              "name": "punctuation.definition.separator.nmo"
            },
            "6": {
              "name": "entity.name.tag.nmo"
            },
            "7": {
              "patterns": [
                {
                  "comment": "separator",
                  "match": "\\b[\\|\\|]\\s?",
                  "name": "punctuation.separator.array.nmo"
                },
                {
                  "comment": "str-value",
                  "match": "([^\\|\\|]+)",
                  "captures": {
                    "1": {
                      "name": "string.nmo"
                    }
                  }
                }
              ]
            },
            "8": {
              "name": "punctuation.definition.separator.nmo"
            },
            "9": {
              "name": "string.nmo"
            }
          }
        }
      ]
    }
  },
  "scopeName": "source.nmo"
}